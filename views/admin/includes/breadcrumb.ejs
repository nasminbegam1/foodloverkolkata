    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark"><%= breadcrumb.title %></h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              
              <% breadcrumb.right.forEach(function(data, i) { %>
                <% if( breadcrumb.right.length ==  i +1  ){ %>
                <li class="breadcrumb-item active"><%= data.title %></li>
                <% } else{ %>  
                <li class="breadcrumb-item"><a href="<%= data.href %>"><%= data.title %></a></li>
                <% } %>
              <% }); %>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->